<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from ApprovalServiceService WSDL service -->

  <service name="ApprovalService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://soap.services.bachelor.fhaachen.de"/>
      <parameter name="wsdlServiceElement" value="ApprovalServiceService"/>
      <parameter name="schemaQualified" value="http://soap.services.bachelor.fhaachen.de"/>
      <parameter name="wsdlServicePort" value="ApprovalService"/>
      <parameter name="className" value="de.fhaachen.bachelor.services.soap.ApprovalServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="ApprovalService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="isApproved" qname="operNS:isApproved" xmlns:operNS="http://soap.services.bachelor.fhaachen.de" returnQName="retNS:isApprovedReturn" xmlns:retNS="http://soap.services.bachelor.fhaachen.de" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:prename" xmlns:pns="http://soap.services.bachelor.fhaachen.de" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:name" xmlns:pns="http://soap.services.bachelor.fhaachen.de" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="isApproved"/>

  </service>
</deployment>
